/* 记事本 */
/* pages/home/home.wxss */
  /* 事件功能部分的整体布局 */
  .outnotepadall{
    border: 1px solid #E6E8EE;
    position: absolute;
    width: 90%;
    height: 700rpx;
    border-radius: 17px;
    background: #ffffff;
    left: 5%;
    top: 700rpx;
    box-shadow: 0px 10px 20px rgba(76, 93, 105, 0.1);
  
  }
  .notepadall{
    position: relative;
    width: 100%;
    height: 100%;
    border-radius: 17px;
    background: #ffffff9a;
  }
    /* 每个事件整体模块的设置 */
  .everyevent{
      position: static;
      width: 90%;
      height: 120rpx;
      margin-left: 5%;
      margin-top: 30rpx;
      /* border: 1rpx solid antiquewhite; */
  }
    /* 每个事件左边的竖线的设置 */
  .line{
    position: absolute;
    width: 4px;
    height: 100rpx;
    margin-top: 15rpx;
    background: #4E11D1;
    border-radius: 2px;
  }
    /* 事件名的设置 */
  .eventname{
    position: absolute;
    margin-left: 8%;
    margin-top: 23rpx;
  
    /* font-family: 'Advent Pro'; */
    font-style: normal;
    font-weight: 400;
    font-size: 20px;
    line-height: 18px;
  
    color: #000000;
  
  }
    /* 距离结束的时间的设置 */
  .deadline{
    position: absolute;
    margin-left: 8%;
    margin-top: 75rpx;
    font-family: 'ABeeZee';
    font-style: normal;
    font-weight: 400;
    font-size: 16px;
    line-height: 14px;
  
    color: rgba(0, 0, 0, 0.5);
  }
    /* 每一个事件下面的横线 */
  .bottomline{
    position: relative;
    left: 9%;
    width: 80%;
    height: 1%;
    background-color: #DCDCDC;
    top: 100%;
    line-height: 14px;
 
  }
  
  
  