.tableTop {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding: 5px;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
  justify-content: space-between;
  height: 30px
}

.week {
  -webkit-align-self: center;
  align-self: center;
  margin-left: 10px
}

.pre,
.next,
.refresh {
  height: 100%;
  margin-left: 10px;
  -webkit-align-self: center;
  align-self: center;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  width: 30px
}

.line {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex
}

.timetableHide {
  min-height: 130px;
  margin: 0 1.5px;
  word-break: break-all;
  color: #fff;
  padding: 3px;
  background: #fff;
  font-size: 12px;
  border-radius: 2px;
  height: 100%;
}

.timetableHide>view {
  margin-bottom: 3px
}

.timetablehr {
  margin: 3px 0;
  background-color: #eee !important;
  height: 1px;
  
}

.weekUnit {
  text-align: center;
  padding: 5px 0 1px 0;
  font-size: 10px;
  width: 100%
}

.weekUnit view {
  padding: 3px 0
}

.adAdapt {
  box-sizing: border-box;
  padding: 5px
}

.operate {
  -webkit-align-self: center;
  align-self: center
}

.none {
  border: none;
  font-size: 8px
}

.todayLine {
  font-size: 8px;
  border-bottom: 3px solid #eee
}





